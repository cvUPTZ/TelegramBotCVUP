{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/webhook.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}